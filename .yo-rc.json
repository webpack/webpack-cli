{
  "webpack-addons-demo": {
    "configuration": {
      "dev": {
        "webpackOptions": {
          "entry": "ok",
          "output": {
            "filename": "[name].js"
          },
          "context": "path.join(__dirname, \"src\")",
          "plugins": [
            "new webpack.optimize.CommonsChunkPlugin({name:'k',filename:'k-[hash].min.js'})"
          ]
        },
        "topScope": [
          "const path = require(\"path\")",
          "const webpack = require(\"webpack\")"
        ],
        "configName": "pengwings"
      }
    }
  }
}